<div id="player-login">
  <%= form_for @session do |f|%>
    <div class="field">
      <%= f.label :account %><br>
      <%= f.text_field :account %>
    </div>
    <div class="field">
      <%= f.label :password %><br>
      <%= f.text_field :password %>
    </div>
    <%= f.submit nil, :id => "submit", :name => "submit", :class => "form_submit", data: { disable_with: tf(:processing) } %>
  <% end -%>
</div>
