<%#image_tag 'test.jpg' %>
<!-- <div id="phaser-example">
</div> -->
<div id="main-header"></div>
<div id="main-background"></div>
<div id="content">
  <div id="menu">
    <div id="main-menu-header">教师系统</div>
    <div id="main-menu">
      <div id="menu-tree">
        <div class="tree-base-header"></div>
        <div class="tree-base-body">
          <div class="tree-select active"></div>
          <div class="tree-select"></div>
          <div class="tree-select"></div>
          <div class="tree-select"></div>
          <div class="tree-select"></div>
        </div>
        <div class="tree-base-header"></div>
      </div>
      <div id="select-titles">
        <div class="select-title active">实验项目</div>
        <div class="select-title">新建实验</div>
        <div class="select-title">实验详情</div>
        <div class="select-title">实验报告</div>
        <div class="select-title">结果分析</div>
      </div>
    </div>
  </div>
  <div id="right-content">
    <div id="main-nav">
      <div id="nav-mark"></div>
      <div id="nav-title">实验项目</div>
      <div id="nav-logout">注销</div>
    </div>

    <div id="experiment-container">

      <div id="experiment-list-container">
      
      <% for i in 1..6 do  %>
        <div class="experiment-cell">
          <div class="experiment-cell-title">实验六</div>
          <div class="experiment-cell-detail">
            <span class="experiment-cell-detail-name">普洛普理论验证</span>
            <span class="experiment-cell-progressbar">
              <div class="experiment-cell-progressbar-body">
                <%# http://demo.doyoe.com/css/inline-block-space/ %>
                <div class="experiment-cell-progressbar-circle active">1</div><div class="experiment-cell-progressbar-circle">2</div><div class="experiment-cell-progressbar-circle">3</div>
              </div>
              <div class="experiment-cell-progressbar-step">待实验</div>
              <div class="experiment-cell-progressbar-step">进行中</div>
              <div class="experiment-cell-progressbar-step">已完成</div>
            </span>
            <span class="experiment-cell-teacher">教师：王小帅</span>
            <span class="experiment-cell-date"><%= l Date.today %></span>
            <span class="experiment-cell-remove"><i class="icon-remove"></i></span>
          </div>
        </div>
      <% end %>

      </div>
    </div>
  </div>
</div>